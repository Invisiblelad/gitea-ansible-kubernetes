apiVersion: apps/v1
kind: Deployment
metadata:
  name: web
  namespace: "{{ namespace }}"
  labels:
    app: web
spec:
  replicas: 2
  selector:
    matchLabels:
      app: web
  template:
    metadata:
      labels:
        app: web
    spec:
      containers:
      - name: web
        image: "{{ image_name }}"
        ports:
        - containerPort: 80

